{
  "expo": {
    "name": "Moodify",
    "slug": "Moodify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icondark.png",
    "scheme": "moodify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icondark.png",
        "backgroundColor": "#EEEED0"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icondark.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
    [
       "expo-font",
       {
        "fonts": [
          "./assets/fonts/LeagueSpartan-Bold.ttf",
          "./assets/fonts/LeagueSpartan-Regular.ttf",
          "./assets/fonts/LaoSansPro-Regular.ttf"
        ]
       }
    ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
